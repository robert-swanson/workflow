FILEPATH=/home/CS/users/rswanson/.linux/cos436-parallelanddistributed/genome

PATTERN=$1
shift
FILES=$* 

echo "PATTERN: $PATTERN, THREAD: $THREADS, FILES: $FILES"

echo "Single Thread:"
$FILEPATH/handout/sg -g 4 -p $PATTERN $FILES
echo ""

for I in 1 2 3 4 5 6 12 24; do
    echo "$I" threads:
    $FILEPATH/genome -g 4 -n $I -p $PATTERN $FILES
    echo ""
done

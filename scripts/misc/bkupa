if [ ! -d "/Volumes/Amaranth/" ]; then
    echo Amaranth is not mounted
    exit
else
    echo Go Ahead
fi
SRC=/Volumes/Amaranth/
DST=/Users/robertswanson/OneDrive\ -\ Taylor\ University/New\ Amaranth\ Backup
EXCLUDE=/Volumes/Amaranth/skip.txt 

echo rsync -a --stats --human-readable --exclude-from="$EXCLUDE" "$SRC" "$DST"
rsync -a --stats --human-readable --exclude-from="$EXCLUDE" "$SRC" "$DST"
